# insertion sort 
# awl esq 1
fun insertionSort(array){
  for( i in 1->arrayLength(array)){
    let j= i-1
	 let  key = array[i]
	 whl( j>=0  && array[j] > key ){
		  array[j+1]=array[j]
		  j=j-1
	 }
    array[j+1]=key
  }
}




let a = [1,3,2,5,7,6]
insertionSort(a)
printOut(a)

fun factorial(n){
    if( n == 0) {
	 return  1 
    } 
	 return  n*factorial(n-1)
}


