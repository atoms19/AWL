# fun awl program 

fun insertionSort(array){
  for( i in 1->arrayLength(array)){
    let j= i-1
	 let  key = array[i]
	 while( j>=0  && array[j] > key ){
		  array[j+1]=array[j]
		  j=j-1
	 }
    array[j+1]=key
  }
}

let a = []
insertionSort(a)
printOut(a)


# factorial 
fun factorial(n){
    if( n == 0) return  1 
	 return n*factorial(n-1)
}

printOut(factorial(5))
